local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/10x00/Tools/main/Venyx%20UI"))()

local library = library.new("Boku No Shit", 5013109572)

--//Vars\\--
local Quest = {};
for i,v in pairs(game:GetService("ReplicatedStorage").Modules.Quests:GetChildren()) do
table.insert(Quest,v.Name)
end;

local Mobs = {};
for i,v in pairs(game:GetService("Workspace").NPCs:GetChildren()) do
table.insert(Mobs,v.Name)
end;

local Speed = 5

--//NoClip\\--
game:GetService("RunService").Stepped:Connect(function()
    if getgenv().Autofarm then
        game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
    end
end)

local QuestNum = "";
local MobToFarm = "";
local DIS = 5;
local PlayerName = game.Players.LocalPlayer.Name

--//Info\\--
tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)


--//Def\\--
getgenv().Autofarm = false;
getgenv().AutoQ = false;

--//Page Starting\\--
local page = library:addPage("Main", 5012544693)

local section1 = page:addSection("Autofarms")
local section2 = page:addSection("Settings")

section1:addDropdown("Quest",Quest,function(value)
QuestNum = value
end);

section1:addDropdown("Mob",Mobs,function(value)
MobToFarm = value
end);

section1:addToggle("Autofarm",nil,function(value)
    getgenv().Autofarm = value

    while getgenv().Autofarm and wait() do
    pcall(function()
        for i,v in pairs(game:GetService("Workspace").NPCs:GetChildren()) do
            if game.Players.LocalPlayer.Character and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") then
                if v.Name == MobToFarm and v.Humanoid.Health > 0 then
                    repeat wait()
                        tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(v.HumanoidRootPart.Position + v.HumanoidRootPart.CFrame.lookVector * DIS)})
                        tween:Play()
                        game:GetService("Players").LocalPlayer.Character.Quirk8.E:FireServer()
                        until v.Humanoid.Health <= 0 or not getgenv().Autofarm
                    end;
                end;
            end;
        end);
    end;
end);

section1:addToggle("Auto Quest",nil,function(value)
    getgenv().AutoQ = value

    while getgenv().AutoQ and wait() do
            game:GetService("ReplicatedStorage").Remotes.Quest.AcceptQuest:FireServer(QuestNum)
    end;
end);

section1:addToggle("Auto Complete Quest",nil,function(value)
    getgenv().C = value

    while getgenv().C and wait() do
    if game:GetService("Workspace").Quests[PlayerName][QuestNum].AmountKilled.Value == game:GetService("Workspace").Quests[PlayerName][QuestNum].AmountNeeded.Value then
        game:GetService("ReplicatedStorage").Remotes.Quest.CompleteQuest:FireServer(QuestNum)
        end;
    end;
end);

section2:addSlider("Distance",0, -10, 10,function(value)
DIS = value
end);

section2:addToggle("Hide Name",nil,function(value)
    getgenv().HideName = value

    while getgenv().HideName and wait() do
        for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v.Name == 'Pants' or v.ClassName == "Accessory" or v.Name == 'Shirt' or v.Name == 'OverHead' or v.Name == "face" then
                v:Destroy()
            end;
        end; 
    end;
end);

section2:addToggle("Invisibilty",nil,function(value)
while value and wait() do
pcall(function()
game.Players.LocalPlayer.Character.LowerTorso:Destroy()
end);
end;
end);
